{
  "name": "netbrowser",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "vite:dev": "vite ./src",
    "vite:build": "vite build ./src --emptyOutDir",
    "app": "npm run vite:build && electron .",
    "app:build": "npm run vite:build && electron-builder"
  },
  "keywords": [],
  "author": "",
  "license": "UPL-1.0",
  "description": "",
  "chromium": "140.0.7339.133",
  "build": {
    "publish": null,
    "appId": "com.unsetsoft.net",
    "productName": "NetBrowser",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "www/**/*",
      "main.js",
      "preload.js",
      "package.json"
    ],
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "allowToChangeInstallationDirectory": false
    }
  },
  "devDependencies": {
    "@eslint/js": "10.0.0",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitejs/plugin-react": "5.0.3",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "electron": "^31.7.7",
    "eslint": "9.36.0",
    "eslint-plugin-react-hooks": "6.0.0",
    "eslint-plugin-react-refresh": "0.4.22",
    "globals": "16.4.0",
    "electron-builder": "26.0.20",
    "vite": "7.1.7"
  },
  "dependencies": {
    "crypto": "1.0.1",
    "hotkeys-js": "3.13.15",
    "react": "19.1.1",
    "react-dom": "19.1.1"
  }
}
